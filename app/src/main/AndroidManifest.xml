<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ab.sync">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Sync">
        <activity android:name=".VideoChatActivity"></activity>
        <activity android:name=".LoadingActivity" />
        <activity android:name=".View.DatabaseActivity" />
        <activity android:name=".CallingActivity" />
        <activity android:name=".FriendProfileActivity" />
        <activity android:name=".profileActivity" />
        <activity android:name=".FindPeopleActivity" />
        <activity android:name=".NotificationActivity" />
        <activity android:name=".auth.LoginActivity" />
        <activity android:name=".View.SplashScreenActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ContactsActivity" />
    </application>

</manifest>